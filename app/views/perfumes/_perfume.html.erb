<div class="card">
  <%= link_to image_tag(perfume.image, class: "card_img"), perfume_path(perfume.id) %>
  <div class="card_body">
    <%= link_to perfume[:perfumename], perfume_path(perfume.id), class: :card_title %>
    <%= link_to "by#{perfume.user.name}", "/users/#{perfume.user.id}", class: :card_user %>
  </div>
</div>